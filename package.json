{
  "name": "sparks-schemas",
  "version": "1.0.6",
  "description": "",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "schemas:data": "rm -rf schemas/data/* && bin/dataFromModels.js",
    "dts": "tsc && node_modules/.bin/dts-generator --out index.d.ts --project ./ --name sparks-schemas --exclude \"typings/**/*.ts\" --exclude \"node_modules/**/*.ts\"",
    "build": "npm run schemas:data && npm run dts",
    "test": "node bin/validate.js"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run build && git add index.d.ts && git add schemas/data"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ajv": "^4.7.7",
    "glob": "^7.1.1",
    "inflection": "^1.10.0"
  },
  "devDependencies": {
    "cli-color": "^1.1.0",
    "dts-generator": "^1.7.0",
    "ghooks": "^1.3.2",
    "json-schema-to-typescript": "^2.4.1",
    "typescript": "^2.0.3",
    "typings": "^1.4.0",
    "typson": "^0.6.0"
  }
}
